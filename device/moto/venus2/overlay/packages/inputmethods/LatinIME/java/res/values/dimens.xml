    <dimen name="keyboard_vertical_correction">0.0in</dimen>

    <fraction name="key_letter_ratio">0%</fraction>
    <fraction name="key_large_letter_ratio">0%</fraction>
    <fraction name="key_label_ratio">0%</fraction>
    <fraction name="key_hint_letter_ratio">0%</fraction>
