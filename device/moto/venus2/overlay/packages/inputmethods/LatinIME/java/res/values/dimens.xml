    <dimen name="key_preview_offset">0px</dimen>

    <dimen name="key_label_horizontal_padding">0px</dimen>
    <dimen name="key_hint_letter_padding">0px</dimen>
    <dimen name="key_popup_hint_letter_padding">0px</dimen>
    <dimen name="key_uppercase_letter_padding">0px</dimen>

    <dimen name="key_preview_height_ics">0px</dimen>
