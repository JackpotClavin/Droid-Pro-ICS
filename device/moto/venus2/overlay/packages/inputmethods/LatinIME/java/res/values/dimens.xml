<resources>
    <!-- keyboardHeight = row_height*4 + key_bottom_gap*3 -->
    <dimen name="keyboardHeight">0px</dimen>
    <fraction name="maxKeyboardHeight">0%p</fraction>
    <fraction name="minKeyboardHeight">0%p</fraction>

    <dimen name="popup_key_height">0px</dimen>

    <dimen name="mini_keyboard_horizontal_edges_padding">0px</dimen>
    <dimen name="mini_keyboard_key_horizontal_padding">0px</dimen>

    <fraction name="keyboard_top_padding">0%p</fraction>
    <fraction name="keyboard_bottom_padding">0%p</fraction>
    <fraction name="keyboard_horizontal_edges_padding">0%p</fraction>
    <fraction name="key_bottom_gap">0%p</fraction>
    <fraction name="key_horizontal_gap">0%p</fraction>

    <dimen name="keyboardHeight_stone">0px</dimen>
    <fraction name="keyboard_top_padding_stone">0%p</fraction>
    <fraction name="keyboard_bottom_padding_stone">0%p</fraction>
    <fraction name="key_bottom_gap_stone">0%p</fraction>
    <fraction name="key_horizontal_gap_stone">0%p</fraction>

    <fraction name="key_bottom_gap_gb">0%p</fraction>
    <fraction name="key_horizontal_gap_gb">0%p</fraction>

    <fraction name="keyboard_top_padding_ics">0%p</fraction>
